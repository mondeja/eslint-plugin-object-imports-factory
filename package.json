{
  "name": "eslint-plugin-object-imports-factory",
  "version": "0.1.1",
  "description": "ESLint plugin to enforce good practices using object factories to import modules.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "object",
    "imports",
    "factory"
  ],
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava",
    "format": "npm run prettier -- --write && npm run xo:fix",
    "xo:fix": "xo --fix",
    "prettier": "prettier --cache --ignore-unknown \"**/*.!(js|jsx|mjs|cjs|ts|tsx|mts|cts|svg)\""
  },
  "author": "Álvaro Mondéjar Rubio",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^9.9.0"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "devDependencies": {
    "ava": "^6",
    "eslint-plugin-eslint-plugin": "^6",
    "xo": "^0.59"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mondeja/eslint-plugin-object-imports-factory.git"
  },
  "bugs": {
    "url": "https://github.com/mondeja/eslint-plugin-object-imports-factory/issues",
    "email": "mondejar1994@gmail.com"
  }
}
